{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "outDir": "./dist",
    "strict": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "lib": ["dom", "webworker"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}